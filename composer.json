{
    "name": "pomm-project/pomm-symfony-bridge",
    "type": "library",
    "description":  "Pomm2 shared elements between Silex and Symfony.",
    "keywords": ["silex", "symfony","pomm", "debug", "profiler"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "Grégoire HUBERT", "email": "hubert.greg@gmail.com" }, { "name": "Jérôme MACIAS", "email": "jerome.macias@gmail.com" } ],
    "require": {
        "php": ">=8.1",
        "pomm-project/foundation": "3.0.0",
        "symfony/http-foundation": "~6.0",
        "symfony/http-kernel": "~6.0",
        "symfony/serializer": "~6.0",
        "symfony/property-info": "~6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.7"
    },
    "suggest": {
        "pomm-project/model-manager": "3.0.0",
        "pomm-project/cli": "3.0.0",
        "symfony/symfony": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\SymfonyBridge\\": "sources/lib"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DoudDNC/Foundation"
        }
    ]
}
